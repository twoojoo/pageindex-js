{
  "name": "pageindex-js",
  "description": "A Javascript/Typescript client for PageIndex",
  "version": "0.0.5",
  "author": "twoojoo",
  "license": "MIT",
  "keywords": [
    "PageIndex",
    "Agent",
    "Search",
    "PDF"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twoojoo/pageindex-js.git"
  },
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "form-data": "^4.0.5",
    "openai": "^6.9.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "dotenv": "^17.2.3",
    "typescript": "^5.9.3"
  }
}
